{
	"name": "__MSG_extName__",
	"short_name": "__MSG_extName__",
	"version": null,
	"description": "__MSG_description__",
	"homepage_url": "http://team.firefoxcn.net",
	"manifest_version": 2,
	"icons": {
		"128": "assets/images/128.png"
	},
	"permissions": [
		"tabs",
		"webRequest",
		"webRequestBlocking",
		"contextMenus",
		"storage",
		"downloads",
		"*://*/*",
		"unlimitedStorage"
	],
	"content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self';",
	"background": {
		"page": ["background.html"]
	},
	"browser_action": {
		"default_icon": {
			"128": "assets/images/128.png"
		},
		"default_title": "__MSG_extButtonTitle__",
		"default_popup": "popup.html"
	},
	"default_locale": "en",
	"options_ui": {
		"page": "options.html",
		"open_in_tab": true
  },
  "__amo__applications": {
    "gecko": {
      "id": "headereditor-amo@addon.firefoxcn.net",
      "strict_min_version": "52.0"
    }
  },
  "__xpi__applications": {
    "gecko": {
      "id": "headereditor@addon.firefoxcn.net",
      "strict_min_version": "52.0",
      "update": "https://ext.firefoxcn.net/headereditor/install/update.json"
    }
  }
}